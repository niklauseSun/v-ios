# platform :ios, '9.0'

target 'VApp-ios' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  
  # m1 安装pod
  # arch -x86_64 pod install

  # Pods for VApp-ios
  # 一对一通信
#  pod 'AgoraRtm_iOS'

  pod 'AgoraAudio_iOS'
  pod 'AgoraRtm_iOS'
  pod 'JSONKit'
  
  # 扫码
  pod 'SGQRCode', '~> 3.5.1'

  # 语音通信
  #  pod 'AgoraAudio_iOS'
  pod 'WSProgressHUD'
  pod 'IQKeyboardManager' #iOS8 and later
  
  # 请求库
  pod 'AFNetworking'
  
  # 图片选择库
  pod 'TZImagePickerController' #iOS8 and later
  # 图片库
  pod 'SDWebImage'
end
